parameters:
  level: 6
  paths:
    - src
  excludePaths:
    - vendor
  ignoreErrors:
    # Laravel Builder generic type - common in Laravel packages
    - identifier: missingType.generics
    # Auth facade magic method - auth() returns Factory but proxies to Guard via __call
    - '#Call to an undefined method Illuminate\\Contracts\\Auth\\Factory::user\(\)#'
